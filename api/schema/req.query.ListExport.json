{
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "limit": {
            "type": "integer",
            "default": 100,
            "maximum": 100,
            "description": "Limit number of returned runs"
        },
        "page": {
            "type": "integer",
            "description": "Page of results to return"
        },
        "status": {
            "$ref": "./util/status.json"
        },
        "before": {
            "type": "string",
            "description": "Only show runs before the given date"
        },
        "after": {
            "type": "string",
            "description": "Only show runs after the given date"
        },
        "uid": {
            "type": "string",
            "description": "The User ID to show exports for - useful for admin only - user's can ownly see their own"
        }
    }
}
