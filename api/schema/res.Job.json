                "output",
                "preview"
            ],
            "additionalProperties": false,
            "properties": {
                "cache": {
                    "type": "boolean"
                },
                "output": {
                    "type": "boolean"
                },
                "preview": {
                    "type": "boolean"
                }
            }
        },
        "loglink": {
            "$ref": "./util/loglink.json"
        },
        "status": {
            "$ref": "./util/status.json"
        },
        "stats": {
            "type": "object"
        },
        "count": {
            "type": ["null", "integer"]
        },
        "bounds": {
            "$ref": "./util/bounds.json"
        },
        "version": {
            "type": "string",
