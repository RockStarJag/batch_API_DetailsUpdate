{
    "type": "object",
    "required": [
        "id",
        "s3",
        "license",
        "run",
        "map",
        "created",
        "source",
        "source_name",
        "layer",
        "name",
        "output",
        "loglink",
        "status",
        "stats",
        "count",
        "bounds",
        "version",
        "size"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "integer"
        },
        "license": {
            "type": ["boolean", "object"]
        },
        "s3": {
            "type": ["boolean", "string"]
        },
        "run": {
            "type": "integer"
        },
        "map": {
            "type": ["null", "integer"]
        },
        "created": {
            "type": "integer"
        },
        "source": {
            "type": "string"
        },
        "source_name": {
            "type": "string"
        },
        "layer": {
